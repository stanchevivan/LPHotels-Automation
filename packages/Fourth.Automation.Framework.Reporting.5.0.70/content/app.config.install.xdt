<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <configSections xdt:Transform="InsertIfMissing" >
  </configSections>
  
  <configSections>
	<section name="report" type="Fourth.Automation.Framework.Reporting.Configuration.ReportSection, Fourth.Automation.Framework.Reporting" xdt:Locator="Condition([@name='report'])" xdt:Transform="InsertIfMissing"/>
  </configSections>

 <report reportName="" filePath="" xdt:Locator="Condition([@reportName!='' or @filePath!=''])">
	<capabilities keepScreenshot="" takeScreenshotOnError="" xdt:Transform="Remove"/>
 </report>

 <report reportName="" filePath="">
	<capabilities keepScreenshot="" takeScreenshotOnError="" xdt:Transform="Remove"/>
 </report>
 
  <report reportName="" filePath="" xdt:Transform="InsertIfMissing">
  </report>
  
  <specFlow xdt:Transform="InsertIfMissing">
  </specFlow>

  <specFlow>
    <stepAssemblies xdt:Transform="InsertIfMissing">
    </stepAssemblies>
  </specFlow>
  
  <specFlow>
    <stepAssemblies>
      <stepAssembly assembly="Fourth.Automation.Framework.Reporting" xdt:Locator="Condition([@assembly='Fourth.Automation.Framework.Reporting'])" xdt:Transform="InsertIfMissing"/>
    </stepAssemblies>
  </specFlow>
  
  
</configuration>