<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
 
  <configSections>
	<section name="report" type="Fourth.Automation.Framework.Reporting.Configuration.ReportSection, Fourth.Automation.Framework.Reporting" xdt:Locator="Condition([@name='report'])" xdt:Transform="Remove"/>
  </configSections>

  <configSections xdt:Locator="Condition(count(*) = 0)" xdt:Transform="Remove" />

	<report reportName="" filePath="" xdt:Locator="Condition([@reportName='' and @filePath=''])" xdt:Transform="Remove">
	</report>
 
	<specFlow>
		<stepAssemblies>
		  <stepAssembly assembly="Fourth.Automation.Framework.Reporting" xdt:Locator="Condition([@assembly='Fourth.Automation.Framework.Reporting'])" xdt:Transform="Remove"/>
		</stepAssemblies>
	</specFlow>
  
	<specFlow>
		<stepAssemblies xdt:Locator="Condition(count(*) = 0)" xdt:Transform="Remove"/>
	</specFlow>
	
	<specFlow xdt:Locator="Condition(count(*) = 0)" xdt:Transform="Remove"/>

</configuration>